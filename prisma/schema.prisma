generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Cliente {
  id Int @id @default(autoincrement())
  dni String
  nombres String
  apellidos String
  fecha_nacimiento DateTime
  duenio_propiedad Int
  email String
  direccion String
  ingreso_mensual Decimal
  estado_civil String
  telefono String
}